{
  "name": "pi-ssh-remote",
  "version": "0.0.1",
  "description": "Pi extension that redirects all file operations and commands to a remote host via SSH",
  "keywords": [
    "pi",
    "pi-coding-agent",
    "extension",
    "ssh",
    "remote",
    "development"
  ],
  "author": "Carlos Villela",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/cv/pi-ssh-remote"
  },
  "pi": {
    "extensions": ["./ssh-remote.ts"]
  },
  "scripts": {
    "test": "jest --testPathIgnorePatterns=e2e",
    "test:watch": "jest --watch --testPathIgnorePatterns=e2e",
    "test:coverage": "jest --coverage --testPathIgnorePatterns=e2e",
    "test:e2e": "jest --testPathPattern=e2e --runInBand --testTimeout=120000",
    "test:all": "npm run test && npm run test:e2e"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@mariozechner/pi-coding-agent": ">=0.1.0"
  }
}
